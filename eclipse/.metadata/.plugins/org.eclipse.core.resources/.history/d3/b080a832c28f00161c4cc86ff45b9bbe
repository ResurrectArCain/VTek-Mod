package esy.es.resurrect.items;

import cpw.mods.fml.common.registry.GameRegistry;
import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class Itemreg {
	public static void itemreg()
	{
		initReg();
		registerItem();
	}
	
	public static Item techItem1;
	
	public static void initReg(){
		techItem1 = new Item().setUnlocalizedName("techItem1").setCreativeTab(CreativeTabs.tabMisc);
	}
	
	public static void registerItem(){
		GameRegistry.registerItem(techItem1, techItem1.getUnlocalizedName());
	}
}